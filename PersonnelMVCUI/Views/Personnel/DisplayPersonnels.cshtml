@model  IEnumerable<PersonnelMVCUI.Models.EntityFramework.Personnel>

<h1>Personnel List</h1>

<table class="table table-bordered">
    <thead>
    <th>Name</th>
    <th>Surname</th>
    <th>Date of Birth</th>
    <th>Salary</th>
    </thead>
    <tbody>
        @foreach (var personnel in Model)
        {
            <tr>
                <td>@personnel.FirstName</td>
                <td>@personnel.LastName</td>
                <td>@personnel.DateOfBirth</td>
                <td>@personnel.Salary</td>
            </tr>
        }
    </tbody>
</table>